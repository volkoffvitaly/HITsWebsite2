                        <a href="https://yandex.ru/maps/67/tomsk/category/university_college/184106140/?utm_medium=mapframe&utm_source=maps" style="color:#eee;font-size:12px;position:absolute;top:14px;">ВУЗ в Томске</a>
                        <iframe src="https://yandex.ru/map-widget/v1/?um=constructor%3A1e32620ed2764c9c7daae7da82593244a65ba5218ce01ad0b837b9864e4782c4&amp;source=constructor" width="100%" height="400" frameborder="0"></iframe>
                    </div>
            </div>
        </footer>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
